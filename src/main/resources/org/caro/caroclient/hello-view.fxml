<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="337.0" prefWidth="339.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" prefHeight="17.0" prefWidth="90.0" text="hello world" />
    <Button onAction="#onHelloButtonClick" text="Hello!" />
   <Button fx:id="handlerButtons" mnemonicParsing="false" onAction="#onHi" style="-fx-background-color: #008DDA;" text="click" />
   <Accordion>
     <panes>
       <TitledPane animated="false" text="untitled 1" />
       <TitledPane animated="false" text="untitled 2" />
     </panes>
   </Accordion>
</VBox>
